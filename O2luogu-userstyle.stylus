			& .banner-content
				padding-left: 1em
				padding-right: 1em
			
			// 特殊处理 TODO：单独变量化
			& .l-card.reply-item
				background: Card-bg-color-float !important
		
		.main-container.lside-bar>footer
			background: none !important
			&>hr
